<p id="notice"><%= notice %></p>

<h1>Listing Awards</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Link</th>
      <th>Description</th>
      <th>Notes</th>
      <th>Entry fee</th>
      <th>Prize</th>
      <th>Deadline</th>
      <th>Entrant type</th>
      <th>Medium type</th>
      <th>Content type</th>
      <th>Subject type</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @awards.each do |award| %>
      <tr>
        <td><%= award.name %></td>
        <td><%= award.link %></td>
        <td><%= award.description %></td>
        <td><%= award.notes %></td>
        <td><%= award.entry_fee %></td>
        <td><%= award.prize %></td>
        <td><%= award.deadline %></td>
        <td><%= award.entrant_type %></td>
        <td><%= award.medium_type %></td>
        <td><%= award.content_type %></td>
        <td><%= award.subject_type %></td>
        <td><%= award.organization %></td>
        <td><%= link_to 'Show', award %></td>
        <td><%= link_to 'Edit', edit_award_path(award) %></td>
        <td><%= link_to 'Destroy', award, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Award', new_award_path %>
